{
    "makefile.extensionOutputFolder": "./.vscode",
    "files.associations": {
        "*.leaf": "html",
        "*.stencil": "html",
        "vector": "cpp",
        "iostream": "cpp"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "python.formatting.provider": "none"
}